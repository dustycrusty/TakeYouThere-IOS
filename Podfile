  pod 'DatePickerDialog'
  pod 'TextFieldEffects'
  pod 'SideMenu'
  pod 'Dropper'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'M13Checkbox'
  pod 'Stripe'
  pod 'Alamofire'


  target 'TYTTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TYTUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
